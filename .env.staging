# Staging Environment Variables
# Used for staging deployments on Vercel

# Convex Backend
CONVEX_DEPLOYMENT="staging-your-deployment"
NEXT_PUBLIC_CONVEX_URL="https://your-staging-deployment.convex.cloud"

# WorkOS Authentication
WORKOS_API_KEY="sk_test_staging_..."
WORKOS_CLIENT_ID="client_staging_..."
WORKOS_REDIRECT_URI="https://staging.yourdomain.com/api/auth/callback"
NEXT_PUBLIC_WORKOS_CLIENT_ID="client_staging_..."
NEXT_PUBLIC_SITE_URL="https://staging.yourdomain.com"

# Admin Users (comma-separated emails)
ADMIN="admin@example.com,staging@example.com"

# Liveblocks (Collaborative Editing)
LIVEBLOCKS_SECRET_KEY="sk_staging_..."
NEXT_PUBLIC_LIVEBLOCKS_PUBLIC_KEY="pk_staging_..."

# ElevenLabs (Voice AI)
ELEVENLABS_API_KEY="sk_staging_..."

# Email
RESEND_FROM="staging@yourdomain.com"
RESEND_TOKEN="re_staging_..."

# Payments (Test Mode)
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_test_..."

# Monitoring & Analytics
BETTERSTACK_API_KEY="bttr_staging_..."
BETTERSTACK_URL="https://staging.betterstack.com"
NEXT_PUBLIC_GA_MEASUREMENT_ID="G-STAGING..."
NEXT_PUBLIC_POSTHOG_KEY="phc_staging_..."
NEXT_PUBLIC_POSTHOG_HOST="https://app.posthog.com"

# Feature Flags & Security
FLAGS_SECRET="flags_staging_..."
ARCJET_KEY="ajkey_staging_..."
SVIX_TOKEN="svix_staging_..."

# CMS
BASEHUB_TOKEN="bshb_staging_..."

# Notifications
KNOCK_API_KEY="sk_staging_..."
KNOCK_FEED_CHANNEL_ID="staging_..."
KNOCK_SECRET_API_KEY="sk_staging_..."
NEXT_PUBLIC_KNOCK_API_KEY="pk_staging_..."
NEXT_PUBLIC_KNOCK_FEED_CHANNEL_ID="staging_..."

# URLs
VERCEL_PROJECT_PRODUCTION_URL="https://staging.yourdomain.com"
NEXT_PUBLIC_APP_URL="https://staging.yourdomain.com"
NEXT_PUBLIC_WEB_URL="https://staging-web.yourdomain.com"
NEXT_PUBLIC_DOCS_URL="https://staging-docs.yourdomain.com"

# Node Environment
NODE_ENV="production"

